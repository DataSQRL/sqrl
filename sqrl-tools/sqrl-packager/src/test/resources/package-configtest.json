{
  "package":
  {
    "name": "datasqrl.examples.Nutshop",
    "version": "1.0.0",
    "variant": "dev",
    "description": "Basic nutshop example"
  },
  "compiler":
  {
    "maxApiArguments": 3
  },
  "engines": {
    "stream": {
      "engineName": "flink",
      "savepoint": false
    },
    "database": {
      "engineName": "jdbc",
      "dbURL": "jdbc:h2:tmp/output",
      "driverName": "org.h2.Driver",
      "dialect": "H2",
      "database": "sqrl"
    }
  },
  "dependencies":
  {
    "datasqrl.examples.Shared": { "version":  "1.0.0", "variant":  "dev" },
    "datasqrl.examples.Basic": { "version":  "1.0.0" }
  }
}
