{
  "version": 1,
  "package":
  {
    "name": "datasqrl.examples.Nutshop",
    "version": "1.0.0",
    "variant": "dev",
    "description": "Basic nutshop example",
    "topics": ["example", "tutorial", "ecommerce"]
  },
  "compiler":
  {
    "addArguments": false,
    "explain": {
      "visual": true,
      "text": false,
      "extended": false,
      "sorted": true
    }
  },
  "engines": {
    "streams": {
      "type": "flink"
    },
    "database": {
      "type": "jdbc",
      "connector": "jdbc",
      "url": "jdbc:h2:tmp/output",
      "driver": "org.h2.Driver",
      "dialect": "H2",
      "database": "sqrl"
    }
  },
  "dependencies":
  {
    "datasqrl.examples.Shared": { "version":  "1.0.0", "variant":  "dev" },
    "datasqrl.examples.Basic": { "version":  "1.0.0" }
  }
}
