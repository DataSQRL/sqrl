{
  "engines": {
    "flink": {
      "config": {
        "execution.attached": true,
        "execution.checkpointing.dir": "file:///data/flink/checkpoints",
        "execution.checkpointing.savepoint-dir": "file:///data/flink/savepoints",
        "execution.target": "local",
        "rest.address": "localhost",
        "rest.port": 8081,
        "table.exec.resource.default-parallelism": 1,
        "taskmanager.memory.network.max": "800m"
      }
    }
  },
  "connectors": {
    "kafka-mutation": {
      "flexible-json.timestamp-format.standard": "ISO-8601"
    },
    "kafka": {
      "flexible-json.timestamp-format.standard": "ISO-8601"
    }
  }
}