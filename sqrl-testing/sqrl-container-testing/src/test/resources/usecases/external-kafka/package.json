{
  "version": "1",
  "enabled-engines": ["flink", "kafka"],
  "connectors": {
    "kafka-mutation": {
      "properties.bootstrap.servers": "${KAFKA_BOOTSTRAP_SERVERS}",
      "properties.group.id": "${KAFKA_GROUP_ID}"
    },
    "kafka": {
      "properties.bootstrap.servers": "${KAFKA_BOOTSTRAP_SERVERS}",
      "properties.group.id": "${KAFKA_GROUP_ID}"
    }
  },
  "test-runner": {
    "delay-sec": -1,
    "required-checkpoints": 1,
    "create-topics": ["input_topic1", "input_topic2"]
  }
}
