IMPORT flink.*; -- Import flink functions
IMPORT data-local.Arrays; -- Import array data

/* Create numeric data source */
UnmodifiedArrayData := SELECT id,
                              timestamp,
                              items
                       FROM Arrays ORDER BY id;


