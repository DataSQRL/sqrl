{
  "version": "1",
  "enabled-engines": [
    "flink",
    "iceberg",
    "snowflake"
  ],
  "engines": {
    "flink": {
      "config": {
        "table.exec.source.idle-timeout": "1 ms"
      },
      "connectors" : {
        "iceberg" : {
          "write.parquet.page-size-bytes": 1000,
          "warehouse":"/tmp/duckdb",
          "catalog-type":"hadoop",
          "catalog-name": "mydatabase"
        }
      }
    }
  },
  "test-runner": {
    "delay-sec": -1
  }
}