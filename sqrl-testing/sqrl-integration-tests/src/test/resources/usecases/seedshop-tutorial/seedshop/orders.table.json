{
  "version": 1,
  "flink" : {
    "format" : "flexible-json",
    "path" : "/data/orders.jsonl",
    "source.monitor-interval" : 1,
    "connector" : "filesystem"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["id", "time"],
    "timestamp" : "time",
    "watermark-millis" : "1"
  }
}