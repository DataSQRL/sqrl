{
  "version": 1,
  "flink" : {
    "format" : "flexible-json",
    "path" : "${DATA_PATH}/schema.jsonl",
    "connector" : "filesystem",
    "source.monitor-interval" : 1

  },
  "table" : {
    "type" : "source",
    "primary-key" : ["_uuid", "event_time"],
    "timestamp" : "event_time",
    "watermark-millis" : "0"
  }
}