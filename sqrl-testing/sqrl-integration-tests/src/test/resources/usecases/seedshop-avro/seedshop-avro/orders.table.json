{
  "version": 1,
  "flink" : {
    "format" : "avro",
    "properties.bootstrap.servers": "${PROPERTIES_BOOTSTRAP_SERVERS}",
    "properties.group.id": "datasqrl-orders",
    "topic": "${sqrl:topic}",
    "connector" : "kafka",
    "avro.timestamp_mapping.legacy" : false
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["id", "time"],
    "timestamp" : "_source_time",
    "watermark-millis" : "0"
  },
  "metadata" : {
    "_source_time" : {
      "attribute" : "timestamp",
      "type": "TIMESTAMP_LTZ(3)"
    }
  }

}