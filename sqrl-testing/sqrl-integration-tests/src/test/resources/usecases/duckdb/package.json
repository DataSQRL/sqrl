{
  "version": "1",
  "enabled-engines": [
    "flink",
    "iceberg",
    "postgres",
    "duckdb",
    "kafka",
    "vertx"
  ],
  "values": {
    "flink-config": {
      "table.exec.source.idle-timeout": "1 ms"
    }
  },
  "engines": {
    "flink": {
      "connectors" : {
        "iceberg" : {
          "warehouse":"/tmp/duckdb",
          "catalog-type":"hadoop",
          "catalog-name": "mydatabase"
        }
      }
    }
  }
}