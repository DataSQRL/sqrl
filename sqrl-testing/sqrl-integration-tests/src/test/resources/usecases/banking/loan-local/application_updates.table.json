{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "/mnt/build/data",
    "source.monitor-interval" : 1,
    "connector" : "filesystem",
    "source.path.regex-pattern" : "to be determined"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["_uuid"],
    "timestamp" : "timestamp",
    "watermark-millis" : "1"
  },
  "metadata" : {
    "_uuid" : {
      "attribute" : "secure.Uuid()"
    }
  }
}