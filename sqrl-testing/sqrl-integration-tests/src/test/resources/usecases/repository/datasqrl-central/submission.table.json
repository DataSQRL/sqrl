{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "/mnt/build/data",
    "source.monitor-interval" : 0,
    "connector" : "filesystem",
    "source.path.regex-pattern" : "to be determined"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["_uuid", "submissionTime"],
    "timestamp" : "submissionTime",
    "watermark-millis" : "1"
  },
  "metadata" : {
    "_uuid" : {
      "attribute" : "secure.Uuid()"
    }
  }
}