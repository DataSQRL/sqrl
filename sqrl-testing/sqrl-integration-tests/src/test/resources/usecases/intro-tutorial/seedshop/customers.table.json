{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "/mnt/build/data",
    "source.monitor-interval" : 0,
    "connector" : "filesystem",
    "source.path.regex-pattern" : "to be determined"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["id", "changed_on"],
    "timestamp" : "timestamp",
    "watermark-millis" : "1"
  },
  "metadata" : {
    "timestamp" : {
      "attribute" : "time.epochMilliToTimestamp(changed_on)"
    }
  }
}