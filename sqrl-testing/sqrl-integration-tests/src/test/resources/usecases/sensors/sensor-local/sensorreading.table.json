{
  "version": 1,
  "flink" : {
    "format" : "csv",
    "path" : "/data/sensorreading.csv.gz",
    "connector" : "filesystem"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["sensorid", "time"],
    "timestamp" : "timestamp",
    "watermark-millis" : "1"
  },
  "metadata" : {
    "timestamp": {
      "attribute": "time.EpochMilliToTimestamp(time)"
    }
  }
}