{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "/data/machinegroup.jsonl.gz",
    "source.monitor-interval" : 1,
    "connector" : "filesystem"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["groupId", "created"],
    "timestamp" : "created",
    "watermark-millis" : "1"
  }
}