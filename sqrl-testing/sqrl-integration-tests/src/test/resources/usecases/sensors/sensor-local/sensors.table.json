{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "/data/sensors.jsonl.gz",
    "source.monitor-interval" : 1,
    "connector" : "filesystem"
  },
  "table" : {
    "type" : "source",
    "primary-key" : ["id", "placed"],
    "timestamp" : "placed",
    "watermark-millis" : "1"
  }
}