{
  "version": 1,
  "flink" : {
    "format" : "json",
    "path" : "file:/mock",
    "source.monitor-interval" : 10000,
    "connector" : "filesystem",
    "source.path.regex-pattern" : ""
  },
  "table" : {
    "type" : "source",
    "timestamp" : "_ingest_time",
    "primary-key" : ["customerid", "email", "name", "lastUpdated"]
  },
  "metadata" : {
    "_ingest_time" : {
      "attribute" : "proctime()"
    }
  }
}