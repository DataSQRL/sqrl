IMPORT ecommerceTs.Customer;
IMPORT ecommerceTs.Orders;
IMPORT ecommerceTs.Product;

JoinStream := SELECT c.*, o.id, o.`time` FROM Orders o
    JOIN Customer c on c.customerid = o.customerid
    JOIN Product p on p.productid = c.customerid;

SelectJoinStream := SELECT customerid, `time` FROM JoinStream;

EXPORT SelectJoinStream TO mysink.CustomerSink;
