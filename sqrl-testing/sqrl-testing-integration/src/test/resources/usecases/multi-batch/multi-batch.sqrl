IMPORT tables.source AS Src;

CREATE TABLE TableA (
  val INT
) WITH (
  'connector' = 'print'
);

CREATE TABLE TableB (
  recordCount INT
) WITH (
  'connector' = 'print'
);

INSERT INTO TableA SELECT * FROM Src;

EXPORT Src TO tables.sink_a;

NEXT_BATCH;

INSERT INTO TableB SELECT * FROM Src;

EXPORT Src TO tables.sink_b;
