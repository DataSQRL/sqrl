{
  "version": "1",
  "enabled-engines": ["flink", "iceberg", "duckdb", "vertx"],
  "script": {
    "main": "duckdb-translation.sqrl"
  },
  "engines": {
    "flink": {
      "config": {
        "execution.checkpointing.interval": "5 s",
        "execution.checkpointing.min-pause": "2 s"
      }
    }
  },
  "connectors": {
    "iceberg": {
      "warehouse": "/tmp/duckdb"
    }
  },
  "test-runner": {
    "delay-sec": -1,
    "required-checkpoints": 1
  }
}
