{
  "version": "1",
  "enabled-engines": [
    "flink",
    "iceberg",
    "postgres",
    "duckdb",
    "kafka",
    "vertx"
  ],
  "script": {
    "main": "duckdb.sqrl"
  },
  "engines": {
    "flink": {
      "config": {
        "parallelism.default": "1",
        "table.exec.source.idle-timeout": "1 ms",
        "execution.checkpointing.interval": "8 s",
        "execution.checkpointing.min-pause": "2 s"
      }
    }
  },
  "connectors": {
    "iceberg": {
      "warehouse": "/tmp/duckdb"
    }
  },
  "test-runner": {
    "delay-sec": -1,
    "required-checkpoints": 1
  }
}
