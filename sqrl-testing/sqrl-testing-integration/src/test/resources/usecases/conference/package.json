{
  "version": "1",
  "enabled-engines": ["vertx", "postgres", "kafka", "flink"],
  "engines": {
    "flink": {
      "config": {
        "table.exec.source.idle-timeout": "1 ms"
      }
    }
  },
  "script": {
    "main": "conference.sqrl"
  },
  "test-runner": {
    "snapshot-folder": "snapshots-conference",
    "delay-sec" : -1,
    "required-checkpoints" : 1
  }
}
