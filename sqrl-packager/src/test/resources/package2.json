{
  "package":
  {
    "name": "datasqrl.examples.Nutshop",
    "version": "1.0.0",
    "variant": "dev",
    "description": "Basic nutshop example"
  },
  "compiler":
  {
    "api":
    {
      "maxArguments": 3
    }
  },
  "engines": [
    {
      "engineName": "flink",
      "savepoint": false
    },
    {
      "engineName": "jdbc",
      "dbURL": "jdbc:h2:tmp/output",
      "driverName": "org.h2.Driver",
      "dialect": "H2",
      "database": "sqrl"
    }
  ]
}
