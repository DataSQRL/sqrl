interface Product {
  productid: Int
  name: String
  description: String
  category: String
}

type ProductSearch implements Product {
  productid: Int
  name: String
  description: String
  category: String
}

type ProductSearchWithId implements Product {
  productid: Int
  name: String
  description: String
  category: String
}

type Query {
  ProductSearch(query: String!): [ProductSearch]
  ProductSearchWithId(query: String!, id: Int!): [ProductSearchWithId]
}

