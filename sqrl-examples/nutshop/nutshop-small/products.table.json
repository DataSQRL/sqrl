    "delimiter": ";",
    "commentPrefix": null,
    "header": [
      "id",
      "name",
      "sizing",
      "weight_in_gram",
      "type",
      "category",
      "usda_id",
      "updated"
    ]
  },
  "name": "products",
  "identifier": "products",
  "connector": {
    "fileURIs": [
      "../sqrl-examples/nutshop/data-small/products.csv"
    ],
    "filenamePattern": "^([^\\.]+?)(?:_part.*)?$",
    "systemType": "dir"
  }
}